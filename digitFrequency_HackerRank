#include<stdio.h>
#include<string.h>
int main() {
    char str[1000];
   scanf("%s",str);
    int count=0;
    int i=0;
   
   char arr[]="0123456789";
       for(int i=0;i<10;i++)
       {
                         
           for(int j=0;j<strlen(str);j++) 
           {
               if(str[j]==arr[i])
               {
               count++;
               }
               else {
               continue;
               }
           }
           printf("%d ",count);
           count=0;
       }   
    return 0;
}
